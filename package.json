{
  "name": "realtyhub",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.18.0 <19.0.0",
    "npm": ">=9.8.0"
  },
  "packageManager": "npm@9.8.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "start": "turbo start",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:migrate": "turbo db:migrate",
    "db:generate": "turbo db:generate"
  },
  "devDependencies": {
    "prettier": "^3.1.0",
    "turbo": "^1.10.16"
  }
} 