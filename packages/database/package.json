{
  "name": "@repo/database",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:push": "prisma db push --skip-generate",
    "db:seed": "ts-node prisma/seed.ts",
    "db:setup": "prisma generate && prisma migrate deploy && ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "6.5.0",
    "bcrypt": "5.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/node": "22.13.10",
    "eslint": "8.55.0",
    "prisma": "5.6.0",
    "tsup": "8.0.1",
    "typescript": "5.3.3"
  }
} 